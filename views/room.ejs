<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Create Room</title>
</head>

<body>
    <h2>Create Room</h2>
    <form action="/chat" method="GET">
        <input type="text" id="title" name="group_title" placeholder="Group Title" autocomplete="off">
        <input type="text" id="group_id" name="group_id" readonly>
        <button>Create Room</button>
    </form>
</body>
<script>
    const group_id = document.getElementById("group_id");
    const id = Math.random().toString().split(".")[1]
    console.log(id);

    // Show ID
    group_id.value = id;
</script>

</html>